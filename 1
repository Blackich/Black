let userName = prompt('Кто вы', '');

if (userName == 'Админ') {
  let pass = prompt('Введите пароль', '');
  if (pass == 'Я главный') {
    alert('Здравствуйте');
  } else if (!pass) {
    alert('Отменено2');
  } else {
    alert('не верный пароль');
  } 
  } else if (!userName) {
    alert('Отменено1');
} else {
  alert('Я вас не знаю');
}
